# Cloudflare Tunnel Client Configuration
# Copy this file to .env and fill in your values

# Your deployed Cloudflare Worker URL
WORKER_URL=https://cloudflare-tunnel-proxy.your-name.workers.dev

# Unique identifier for this agent (alphanumeric, no spaces)
AGENT_ID=my-laptop

# Secret bearer token for authentication
# Generate with: openssl rand -hex 32
TOKEN=your-secret-token-here

# Local service to proxy to (default: http://127.0.0.1:8000)
TARGET=http://127.0.0.1:8000
